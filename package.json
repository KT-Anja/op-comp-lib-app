{
	"name": "op-component-library-react",
	"private": true,
	"version": "0.0.1",
	"files": [
		"dist"
	],
	"main": "./dist/op-comp-lib.umd.js",
	"module": "./dist/op-comp-lib.es.js",
	"typings": "./dist/",
	"exports": {
		".": {
			"import": "./dist/op-comp-lib.es.js",
			"require": "./dist/op-comp-lib.umd.js"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build-html": "tsc && vite build -c vite-html.config.ts",
		"preview": "vite preview",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"dependencies": {
		"@popperjs/core": "^2.11.5",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^6.1.1",
		"@types/bootstrap": "^5.1.10",
		"@types/node": "^17.0.23",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^17.0.10",
		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"@typescript-eslint/parser": "^5.17.0",
		"@vitejs/plugin-react": "^1.0.7",
		"bootstrap": "^5.1.3",
		"classnames": "^2.3.1",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-react": "^7.29.4",
		"prettier": "^2.6.1",
		"react-router-dom": "^6.2.2",
		"react-select": "^5.3.1",
		"sass": "^1.49.9",
		"typescript": "^4.5.4",
		"vite": "^2.8.0"
	}
}
